{
  "datasets": [
    {
      "name": "efd4f133",
      "displayName": "Daily Validation Summary",
      "queryLines": [
        "WITH validation_summary_table AS (\n",
        "    SELECT \n",
        "        * EXCEPT (overall_status),\n",
        "        TO_DATE(\n",
        "            REGEXP_EXTRACT(\n",
        "                iteration_name, \n",
        "                r'_(20\\d{2}(0[1-9]|1[0-2])(0[1-9]|[12][0-9]|3[01]))_', \n",
        "                1\n",
        "            ), \n",
        "            'yyyyMMdd'\n",
        "        ) AS validation_date,\n",
        "        CASE \n",
        "            WHEN row_count_match_status != 'FAILED'\n",
        "             AND schema_match_status != 'FAILED'\n",
        "             AND col_name_compare_status != 'FAILED'\n",
        "             AND data_type_compare_status != 'FAILED'\n",
        "             AND data_reconciliation_status != 'FAILED'\n",
        "            THEN 'PASSED'\n",
        "            ELSE 'FAILED'\n",
        "        END AS overall_status\n",
        "    FROM <<validation_catalog_name>>.<<validation_schema_name>>.validation_summary\n",
        ")\n",
        "SELECT \n",
        "    validation_date,\n",
        "    overall_status,\n",
        "    COUNT(*) AS total_validations\n",
        "FROM validation_summary_table\n",
        "WHERE validation_date >= CURRENT_DATE - INTERVAL '7' DAY\n",
        "GROUP BY \n",
        "    validation_date, \n",
        "    overall_status\n",
        "ORDER BY \n",
        "    validation_date DESC;"
      ],
      "catalog": "cat_ril_nayeem_03",
      "schema": "dmvdb_system"
    },
    {
      "name": "70208dc3",
      "displayName": "Validation Trend Over Time",
      "queryLines": [
        "WITH validation_summary_table AS (\n",
        "    SELECT\n",
        "        *,\n",
        "        TO_DATE(\n",
        "            REGEXP_EXTRACT(\n",
        "                iteration_name, \n",
        "                r'_(20\\d{2}(0[1-9]|1[0-2])(0[1-9]|[12][0-9]|3[01]))_', \n",
        "                1\n",
        "            ),\n",
        "            'yyyyMMdd'\n",
        "        ) AS validation_date,\n",
        "        CASE \n",
        "            WHEN row_count_match_status != 'FAILED'\n",
        "             AND schema_match_status != 'FAILED'\n",
        "             AND col_name_compare_status != 'FAILED'\n",
        "             AND data_type_compare_status != 'FAILED'\n",
        "             AND data_reconciliation_status != 'FAILED'\n",
        "            THEN 'PASSED'\n",
        "            ELSE 'FAILED'\n",
        "        END AS status\n",
        "    FROM <<validation_catalog_name>>.<<validation_schema_name>>.validation_summary\n",
        ")\n",
        "SELECT \n",
        "    validation_date AS run_date,\n",
        "    SUM(CASE WHEN status = 'PASSED' THEN 1 ELSE 0 END) AS successful_validations,\n",
        "    SUM(CASE WHEN status <> 'PASSED' THEN 1 ELSE 0 END) AS failed_validations,\n",
        "    COUNT(*) AS total_validations,\n",
        "    ROUND(100.0 * SUM(CASE WHEN status = 'PASSED' THEN 1 ELSE 0 END) / COUNT(*), 2) AS success_rate_percent\n",
        "FROM validation_summary_table\n",
        "GROUP BY validation_date\n",
        "ORDER BY run_date DESC;\n"
      ]
    },
    {
      "name": "2f454de4",
      "displayName": "Validation Table",
      "queryLines": [
        "SELECT\n",
        "    tgt_table as target_table,\n",
        "    batch_load_id,\n",
        "    TO_DATE(\n",
        "    REGEXP_EXTRACT(\n",
        "        iteration_name, \n",
        "        r'_(20\\d{2}(0[1-9]|1[0-2])(0[1-9]|[12][0-9]|3[01]))_', \n",
        "        1\n",
        "    ),\n",
        "    'yyyyMMdd'\n",
        ") AS validation_date,\n",
        "CASE \n",
        "    WHEN row_count_match_status != 'FAILED'\n",
        "     AND schema_match_status != 'FAILED'\n",
        "     AND col_name_compare_status != 'FAILED'\n",
        "     AND data_type_compare_status != 'FAILED'\n",
        "     AND data_reconciliation_status != 'FAILED'\n",
        "    THEN 'PASSED'\n",
        "    ELSE 'FAILED'\n",
        "  END AS overall_status,\n",
        "    row_count_match_status,\n",
        "    schema_match_status,\n",
        "    col_name_compare_status,\n",
        "    data_type_compare_status,\n",
        "    -- primary_key_compliance_status,\n",
        "    data_reconciliation_status,\n",
        "    metrics.src_records as source_count,\n",
        "    metrics.tgt_records as target_count,\n",
        "    metrics.src_extras as source_extras,\n",
        "    metrics.tgt_extras as target_extras,\n",
        "    metrics.matches as matches,\n",
        "    metrics.mismatches as mismatches,\n",
        "    iteration_name,\n",
        "    workflow_name,\n",
        "    table_family,\n",
        "    src_table as source_table\n",
        "\n",
        "FROM <<validation_catalog_name>>.<<validation_schema_name>>.validation_summary order by validation_date desc;\n"
      ]
    },
    {
      "name": "53988a37",
      "displayName": "Daily Validation counts",
      "queryLines": [
        "WITH validation_summary_table AS (\n",
        "    SELECT \n",
        "        TO_DATE(\n",
        "            REGEXP_EXTRACT(\n",
        "                iteration_name, \n",
        "                r'_(20\\d{2}(0[1-9]|1[0-2])(0[1-9]|[12][0-9]|3[01]))_', \n",
        "                1\n",
        "            ), \n",
        "            'yyyyMMdd'\n",
        "        ) AS validation_date,\n",
        "          CASE \n",
        "    WHEN row_count_match_status != 'FAILED'\n",
        "     AND schema_match_status != 'FAILED'\n",
        "     AND col_name_compare_status != 'FAILED'\n",
        "     AND data_type_compare_status != 'FAILED'\n",
        "     AND data_reconciliation_status != 'FAILED'\n",
        "    THEN 'PASSED'\n",
        "    ELSE 'FAILED'\n",
        "  END AS status\n",
        "    FROM <<validation_catalog_name>>.<<validation_schema_name>>.validation_summary\n",
        "),\n",
        "date_scaffold AS (\n",
        "    SELECT CURRENT_DATE AS validation_date\n",
        ")\n",
        "SELECT \n",
        "    ds.validation_date,\n",
        "    COALESCE(COUNT_IF(vst.status = 'PASSED'), 0) AS total_success,\n",
        "    COALESCE(COUNT_IF(vst.status = 'FAILED'), 0) AS total_failures,\n",
        "    COALESCE(COUNT(vst.status), 0) AS total_validations\n",
        "FROM date_scaffold ds\n",
        "LEFT JOIN validation_summary_table vst \n",
        "    ON ds.validation_date = vst.validation_date\n",
        "GROUP BY ds.validation_date;\n",
        " "
      ]
    }
  ],
